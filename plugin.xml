<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://cordova.apache.org/ns/plugins/1.0"
    id="com.freynolds.SharePlugin"
    version="0.1.0">
    <name>SharePlugin</name>   
    <!-- ios -->
    <platform name="ios">    
        <config-file target="config.xml" parent="/*">
            <feature name="SharePlugin">
                <param name="ios-package" value="SharePlugin" /> 
            </feature>
        </config-file>
        <js-module src="www/SharePlugin.js" name="SharePlugin">
            <clobbers target="SharePlugin" />
        </js-module>
        <header-file src="src/ios/SharePlugin.h" />
	    <source-file src="src/ios/SharePlugin.m" />
    </platform>
</plugin>