<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://cordova.apache.org/ns/plugins/1.0"
    id="com.freynolds.dax"
    version="0.1.1">
    <name>Comscore DAXPlugin</name>   
    <!-- ios -->
    <platform name="ios">    
        <config-file target="config.xml" parent="/*">
            <feature name="DAXPlugin">
                <param name="ios-package" value="DAXPlugin" /> 
            </feature>
        </config-file>
        <js-module src="www/DAXPlugin.js" name="DAXPlugin">
            <clobbers target="DAXPlugin" />
        </js-module>
        <header-file src="src/ios/DAXPlugin.h" />
	    <source-file src="src/ios/DAXPlugin.m" />

        <source-file src="src/ios/comscore/libcomScore.a" framework="true" />
        <header-file src="src/ios/comScore/headers/CSApplicationState.h" />
        <header-file src="src/ios/comScore/headers/CSCacheFlusher.h" />
        <header-file src="src/ios/comScore/headers/CSCensus.h" />
        <header-file src="src/ios/comScore/headers/CSComScore.h" />
        <header-file src="src/ios/comScore/headers/CSCore.h" />
        <header-file src="src/ios/comScore/headers/CSEventType.h" />
        <header-file src="src/ios/comScore/headers/CSKeepAlive.h" />
        <header-file src="src/ios/comScore/headers/CSMeasurementDispatcher.h" />
        <header-file src="src/ios/comScore/headers/CSNotificationsObserver.h" />
        <header-file src="src/ios/comScore/headers/CSOfflineCache.h" />
        <header-file src="src/ios/comScore/headers/CSSessionState.h" />
        <header-file src="src/ios/comScore/headers/CSStorage.h" />
        <header-file src="src/ios/comScore/headers/CSStreamSense.h" />
        <header-file src="src/ios/comScore/headers/CSStreamSenseAVPlayerPlugin.h" />
        <header-file src="src/ios/comScore/headers/CSStreamSenseClip.h" />
        <header-file src="src/ios/comScore/headers/CSStreamSenseDefines.h" />
        <header-file src="src/ios/comScore/headers/CSStreamSenseDelegate.h" />
        <header-file src="src/ios/comScore/headers/CSStreamSenseMPMoviePlayerControllerPlugin.h" />
        <header-file src="src/ios/comScore/headers/CSStreamSensePlaylist.h" />
        <header-file src="src/ios/comScore/headers/CSStreamSensePlugin.h" />
        <header-file src="src/ios/comScore/headers/CSStreamSensePluginProtocol.h" />
        <header-file src="src/ios/comScore/headers/CSStreamSenseState.h" />
        <header-file src="src/ios/comScore/headers/CSStreamingTag.h" />
        <header-file src="src/ios/comScore/headers/CSTaskExecutor.h" />
        <header-file src="src/ios/comScore/headers/CSTransmissionMode.h" />
        <framework src="Security.framework" />  
        <framework src="SystemConfiguration.framework" />
    </platform>
</plugin>